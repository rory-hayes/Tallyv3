# Vercel environment placeholders (safe to import)
# Replace with real values when ready.

DATABASE_URL=postgresql://tally:password@db.example.com:5432/tally
SESSION_SECRET=replace-with-32-plus-characters-strong-secret
APP_BASE_URL=https://tallyv3-web-placeholder.vercel.app

# Optional: only set for S3-compatible storage. For AWS S3 leave unset.
S3_ENDPOINT=
S3_REGION=us-east-1
S3_BUCKET=tally-placeholder
S3_ACCESS_KEY_ID=replace-with-access-key
S3_SECRET_ACCESS_KEY=replace-with-secret-key
S3_SESSION_TOKEN=optional-if-using-temp-credentials
S3_FORCE_PATH_STYLE=false

# Optional: import validation limits
IMPORT_MAX_BYTES=52428800
IMPORT_MAX_ROWS=50000
IMPORT_MAX_COLUMNS=200

# Client-side upload strategy: proxy (default) or direct
NEXT_PUBLIC_UPLOAD_MODE=proxy
